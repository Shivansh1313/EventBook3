<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.inspiron.eventbook1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".splashscreen4"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".login"
            android:label="Register"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".register"
            android:label="Login"
            android:theme="@style/AppTheme.NoActionBar" />
        <!--
             ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".user_area"
            android:label="@string/title_activity_user_area"
            android:parentActivityName=".login"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.inspiron.eventbook1.login" />
        </activity>
        <activity
            android:name=".user_area2"
            android:label="@string/title_activity_user_area2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user_area3"
            android:label="@string/title_activity_user_area3"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".login2"
            android:label="@string/title_activity_login2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".register2"
            android:label="@string/title_activity_register2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".user_area5"
            android:label="@string/title_activity_user_area5"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>